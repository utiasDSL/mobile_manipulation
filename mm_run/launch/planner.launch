<?xml version="1.0"?>
<!-- Launches planner test node for debugging planners. -->

<launch>
  <arg name="config"/>
  <arg name="ctrl_config" default="default" doc="alternative controller config overwriting the one in config"/>
  <arg name="planner_config" default="default" doc="alternative planner config overwriting the one in config"/>
  <arg name="logging_sub_folder" default="default" doc="save data to a sub folder if set"/>

  <!-- Planner Test Node -->
  <node pkg="mm_run" type="planner_test_ros.py" name="planner_test" output="screen" required="true" args="--config $(arg config) --ctrl_config $(arg ctrl_config) --planner_config $(arg planner_config) --logging_sub_folder $(arg logging_sub_folder)"/>
</launch>
